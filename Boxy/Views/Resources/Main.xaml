<ResourceDictionary
    x:Class="Boxy.Views.Resources.Main"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:resources="clr-namespace:Boxy.Views.Resources"
    x:Name="MainResourceDictionary"
    mc:Ignorable="d">
    <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary
            Source="pack://application:,,,/Views/Resources/Views.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Views/Style/Colors.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Views/Style/VectorData.xaml" />
        <ResourceDictionary
            Source="pack://application:,,,/Views/Style/ControlStyles.xaml" />
    </ResourceDictionary.MergedDictionaries>

    <resources:ViewModelLocator
        x:Key="ViewModelLocator" />

</ResourceDictionary>