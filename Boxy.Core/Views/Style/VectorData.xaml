    <!--  Base Styles  -->

    <Style
        x:Key="TranslucentWhenDisabled"
        TargetType="{x:Type Path}">
        <Setter Property="OverridesDefaultStyle" Value="True" />
        <Style.Triggers>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Opacity" Value="0.50" />
            </Trigger>
        </Style.Triggers>
    </Style>

    <Style
        x:Key="GrayWhenDisabled"
        TargetType="{x:Type Path}">
